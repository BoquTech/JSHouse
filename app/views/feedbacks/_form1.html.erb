   <div class="field">
    <%= "姓名："+@feedback.name %>
  </div>
  <div class="field">
    <%= "电话："+@feedback.phone %>
  </div>
  <div class="field">
    <%="电子邮件：" +@feedback.email %>
  </div>
  <div class="field">

    <%= "留言："+@feedback.message %>
  </div>

<%= form_for(@feedback) do |f| %>
  <% if @feedback.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@feedback.errors.count, "error") %> prohibited this feedback from being saved:</h2>

      <ul>
      <% @feedback.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %> 
<div class="field">
    <%= f.label "回复：" %><br>
    <%= f.text_field :message %>
  </div>
  <div class="actions">
    <%= f.submit "提交" %>
  </div>
<% end %>
